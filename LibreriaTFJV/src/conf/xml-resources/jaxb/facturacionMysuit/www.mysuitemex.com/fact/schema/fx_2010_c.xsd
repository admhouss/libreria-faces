<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
  targetNamespace="http://www.fact.com.mx/schema/fx" xmlns="http://www.fact.com.mx/schema/fx"
  xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="FactDocsMX">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" name="FactDocMX" type="TFactDocMX"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="FactDocMX" type="TFactDocMX"/>
  <xs:complexType name="TFactDocMX">
    <xs:sequence>
      <xs:element fixed="4" name="Version" type="xs:integer"/>
      <xs:element name="Identificacion">
        <xs:complexType>
          <xs:sequence>
            <xs:element fixed="MX" name="CdgPaisEmisor" type="TSenderCountryCode">
              <xs:annotation>
                <xs:documentation>Obligatorio MX</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="TipoDeComprobante" type="TTipoDeDocumento">
              <xs:annotation>
                <xs:documentation>Uno de los tipos comunes de comprobante, que se transfroman a ingreso, egreso o traslado. </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="RFCEmisor" type="TRFC"/>
            <xs:element name="RazonSocialEmisor">
              <xs:simpleType>
                <xs:restriction base="TCadenaPermitida">
                  <xs:maxLength value="250"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Usuario">
              <xs:simpleType>
                <xs:restriction base="TCadenaPermitida">
                  <xs:maxLength value="250"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="NumeroInterno">
              <xs:annotation>
                <xs:documentation>Puede informarse otro identificador no fiscal.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="TCadenaPermitida">
                  <xs:maxLength value="250"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="AsignacionSolicitada">
              <xs:annotation>
                <xs:documentation>Solo cfdv3 (cfdi).</xs:documentation>
                <xs:documentation>Si existe, el motor tratará realizar la asignación.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="Serie" type="TSerie"/>
                  <xs:element name="Folio" type="TFolio"/>
                  <xs:element minOccurs="0" name="TiempoDeEmision" type="xs:dateTime">
                    <xs:annotation>
                      <xs:documentation>Si no existe, el motor asignará Now()</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="CadenaOriginal">
              <xs:annotation>
                <xs:documentation>No emplear este elemento.</xs:documentation>
                <xs:documentation>Será incluido automáticamente.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:token">
                  <xs:minLength value="1"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="Cancelaciones">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="100" name="CancelaYSustituye">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="Numero" type="TCode"/>
                  <xs:element minOccurs="0" name="Fecha" type="xs:date"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="Asignacion" type="TAsignacion">
        <xs:annotation>
          <xs:documentation>No emplear este elemento.</xs:documentation>
          <xs:documentation>Solo para algunas implementaciones in-house con cfdv2.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Origen">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" name="Area">
              <xs:annotation>
                <xs:documentation>TWEB, FIA, SUN, etc.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:token">
                  <xs:minLength value="1"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="Modulo">
              <xs:annotation>
                <xs:documentation>DOMICILIADOS, SUBSECUENTES, etc.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:token">
                  <xs:minLength value="1"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="Procesamiento" type="TDictionaries"> </xs:element>
      <xs:element name="Emisor">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="DomicilioFiscal" type="TDomicilioMexicano"/>
            <xs:element minOccurs="0" name="DomicilioDeEmision" type="TDomicilioMexicano"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Receptor">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="CdgPaisReceptor" type="TCountryCode"/>
            <xs:element name="RFCReceptor" type="TRFC"/>
            <xs:element minOccurs="0" name="TaxID" type="TCadenaPermitida">
              <xs:annotation>
                <xs:documentation>Para no mexicanos.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="NombreReceptor">
              <xs:annotation>
                <xs:documentation>Si el emisor es mexicano, se entiende como Razón Social.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="TCadenaPermitida">
                  <xs:maxLength value="250"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Domicilio">
              <xs:complexType>
                <xs:choice>
                  <xs:element name="DomicilioFiscalMexicano" type="TDomicilioMexicano"/>
                  <xs:element name="OtroDomicilio" type="TDomicilio"/>
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="DomicilioDeRecepcion">
              <xs:complexType>
                <xs:choice>
                  <xs:element name="DomicilioFiscalMexicano" type="TDomicilioMexicano"/>
                  <xs:element name="OtroDomicilio" type="TDomicilio"/>
                </xs:choice>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Conceptos">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" name="Concepto">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="Cantidad" type="TQuantity"/>
                  <xs:element name="UnidadDeMedida">
                    <xs:simpleType>
                      <xs:restriction base="TCadenaPermitida">
                        <xs:maxLength value="32"/>
                        <xs:pattern value="[^|]*"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="Codigo">
                    <xs:simpleType>
                      <xs:restriction base="TCadenaPermitida">
                        <xs:maxLength value="32"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="Descripcion">
                    <xs:simpleType>
                      <xs:restriction base="TCadenaPermitida"> </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="ValorUnitario" type="TNonNegativeAmount"/>
                  <xs:element name="Importe" type="TNonNegativeAmount"/>
                  <xs:element minOccurs="0" name="Opciones">
                    <xs:complexType>
                      <xs:choice minOccurs="0">
                        <xs:element minOccurs="0" name="DatosDeImportacion">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element maxOccurs="12" name="InformacionAduanera"
                                type="TInformacionAduanera"/>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                      </xs:choice>
                    </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="ConceptoEx" type="TConceptoEx"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Totales">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Moneda" type="TCurrencyCode"/>
            <xs:element name="TipoDeCambioVenta">
              <xs:simpleType>
                <xs:restriction base="TConvFactor"/>
              </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="1" name="SubTotalBruto" type="TNonNegativeAmount"/>
            <xs:element name="SubTotal" type="TNonNegativeAmount"/>
            <xs:element minOccurs="0" name="DescuentosYRecargos" type="TDescuentosYRecargos"> </xs:element>
            <xs:element minOccurs="1" name="ResumenDeDescuentosYRecargos"
              type="TResumenDeDescuentosYRecargos"/>
            <xs:element minOccurs="0" name="Impuestos" type="TImpuestos"> </xs:element>
            <xs:element minOccurs="1" name="ResumenDeImpuestos" type="TResumenDeImpuestos"/>
            <xs:element name="Total" type="TNonNegativeAmount"/>
            <xs:element name="TotalEnLetra">
              <xs:simpleType>
                <xs:restriction base="TCadenaPermitida">
                  <xs:maxLength value="250"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="FormaDePago">
              <xs:simpleType>
                <xs:restriction base="xs:token">
                  <xs:pattern
                    value="PAGO EN UNA SOLA EXHIBICION|PARCIALIDAD\s[1-9]{1}[0-9]{0,3}\sDE\s[1-9]{1}[0-9]{0,3}"
                  />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="ResumenAduanero" type="xs:token"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="Complementos">
        <xs:complexType>
          <xs:all>
            <xs:element minOccurs="0" name="Divisas" type="TComplementoDivisas"/>
            <xs:element minOccurs="0" name="Donatarias">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="NumeroDeAutorizacion">
                    <xs:simpleType>
                      <xs:restriction base="TCadenaPermitida">
                        <xs:maxLength value="20"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="FechaDeAutorizacion" type="TAllowedDate"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="ComprobanteEx" type="TComprobanteEx"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TEmbalaje">
    <xs:sequence>
      <xs:element minOccurs="0" name="Cantidad" type="xs:positiveInteger"/>
      <xs:element name="Tipo">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="EXCHANGE_PALLETS"/>
            <xs:enumeration value="RETURN_PALLETS"/>
            <xs:enumeration value="PALLET_80x100"/>
            <xs:enumeration value="CASE"/>
            <xs:enumeration value="BOX"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="Descripcion" type="xs:token"/>
      <xs:element name="PagoDeTransporte">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="PREPAID_BY_SELLER"/>
            <xs:enumeration value="PAID_BY_BUYER"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="UnidadDeEmpaque">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:minLength value="1"/>
            <xs:maxLength value="128"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="TAllowedDate">
    <xs:annotation>
      <xs:documentation>A partir de 1900.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:date">
      <xs:minInclusive value="1900-01-01"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TAllowedDateTime">
    <xs:annotation>
      <xs:documentation>A partir de 1900.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:dateTime">
      <xs:minInclusive value="1900-01-01T00:00:00"/>
      <xs:maxInclusive value="9999-12-31T23:59:59"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TBase64">
    <xs:annotation>
      <xs:documentation>Solo carácteres permitidos en la codificación base64.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:pattern value="[A-Za-z0-9+/=]+"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TCode">
    <xs:annotation>
      <xs:documentation>Código alfanumérico de uso general. [A-Z0-9]{1,32}.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:maxLength value="64"/>
      <xs:minLength value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TCommercialRelationship">
    <xs:annotation>
      <xs:documentation>Proveedor o contratista.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="PROVIDER"/>
      <xs:enumeration value="SUPPLIER"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TConvFactor">
    <xs:annotation>
      <xs:documentation>Tipo de cambio de venta, dos a seis decimales, mayor que cero, para llegar a moneda nacional por multiplicación.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:minExclusive value="0.0"/>
    </xs:restriction>
  </xs:simpleType>
